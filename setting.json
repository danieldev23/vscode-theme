{
  "workbench.colorTheme": "Evondev Dracula Normal Constrast",
  "editor.fontFamily": "SF Mono",
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.lineHeight": 30,
  "editor.letterSpacing": 0.8,
  "editor.renderLineHighlight": "none",
  "editor.fontSize": 21,
  "editor.formatOnSave": true,
  "tabnine.experimentalAutoImports": true,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.fontFamily": "MesloLGL NF",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "liveServer.settings.donotShowInfoMsg": true,
  "vscode_custom_css.imports": [
    "file:///C:/Users/Administrator/Documents/style.css"
  ],
  "editor.cursorSmoothCaretAnimation": true,
  "editor.suggest.preview": true, 
  // "javascript.inlayHints.parameterNames.enabled": "all",
  // "typescript.inlayHints.parameterNames.enabled": "all",
  "[html]": {
    "editor.matchBrackets": "never"
  },
  "[css]": {
    "editor.matchBrackets": "never"
  },
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "[Evondev Dracula Normal Constrast]": {
    "textMateRules": [
      {
        "scope": [
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.jsx",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.tsx",
          "entity.other.attribute-name.html",
          "entity.other.attribute-name.pug",
          "entity.other.attribute-name.php"

        ],
        "settings": {
          "foreground": "#20e4b2"
        }
      }
    ]
  }
}
